<section>
  <%= form_with model: @pet, local: true do |f| %>
    <%= f.label :image_url %>
    <%= f.text_field :image %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :description %>
    <%= f.text_field :description %>
    <%= f.label :approximate_age %>
    <%= f.text_field :approximate_age %>
    <%= f.label :sex %>
    <%= f.text_field :sex %>
    <%= f.submit %>
  <% end %>
</section>
