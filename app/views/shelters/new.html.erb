<h1>New Shelter</h1>

<%= form_with model: @shelter, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :address %>
  <%= f.text_field :address%>
  <%= f.label :city %>
  <%= f.text_field :city%>
  <%= f.label :state %>
  <%= f.select(:state, Shelter.state_abbr) %>
  <%= f.label :zip %>
  <%= f.text_field :zip%>
  <%= f.submit %>
<% end %>
